.tile-slider.tile-slider--place-shadow.tile(data-flickity-options='{ "cellAlign": "left", "prevNextButtons": false, "watchCSS": true, "lazyLoad": true }')
	each item, i in dataServices
		- let id = i + 1;
		.tile__item(data-service=id)
			.tile__icon !{item.icon}
			.tile__title
				a.tile__link(href="#") !{item.title}
			p
				span.-fw-b Объем работ
				span.-c-gray--dark (запросы)

			.tariff-options.tariff-options--wrap

			p.-fw-b Сроки проведения
			.timing 8 рабочих дней

			.tile__footer
				.price-more
					.price-more__old
						- let priceDiscount = item.price * .85;
						span !{item.price}
						| &nbsp;руб.
					.price-more__actual
						span !{priceDiscount}
						| &nbsp;руб.
					.price-more__discount
						.alert.alert--warning Скидка 15%, при оплате онлайн
				button.button.button--primary.button--wide Заказать
				p.-ta-c
					a(href="#") Подробнее
			.tile__note !{item.note}